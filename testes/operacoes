READ R1
READ R2

; and
COPY R0 R1
AND R0 R2
WRITE R0

; or
COPY R0 R1
OR R0 R2
WRITE R0

; not n1
COPY R0 R1
NOT R0
WRITE R0

; xor
COPY R0 R1
XOR R0 R2
WRITE R0

; soma
COPY R0 R1
ADD R0 R2
WRITE R0

; subtracao
COPY R0 R1
SUB R0 R2
WRITE R0

; mutiplicacao
COPY R0 R1
MUL R0 R2
WRITE R0

; divisao
LOAD R3 ZERO
CMP R2 R3
JN FIM

COPY R0 R1
DIV R0 R2
WRITE R0

; resto
COPY R0 R1
MOD R0 R2
WRITE R0

FIM: HALT
ZERO: WORD 0
END